init= {'k1':10,'k2':20,'k3':30,'k4':10,'k5':40,'k6':50}

result={}
print('original dictonary is :\n',init)
for key,value in init.items():
	if value not in result.values():
		result[key]=value
print('\nAfter Removing duplicate values')
print(result)

OUTPUT ::

